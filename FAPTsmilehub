TPTab:AddButton({ Name = "æ°´è‰²ã®å®¶ã«ãƒ†ãƒ¬ãƒãƒ¼ãƒˆ", Callback = function() Teleport(Vector3.new(496.99, 83.34, -350.96)) end })

