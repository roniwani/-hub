    Name = "æœ‰åŠ¹åŒ–",
    Default = false,
    Callback = function(Value)
        IsGeometricActive = Value
        if Value then
            startGeometricLoop()
        else
            if geometricLoopConn then geometricLoopConn:Disconnect() end
        end
    end
})

SparklerTab:AddDropdown({
