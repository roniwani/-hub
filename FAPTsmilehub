Name = "WINGS å¯¾è±¡ãƒ—ãƒ¬ã‚¤ãƒ¤ãƒ¼",
Default = "è‡ªåˆ†",
Options = getPlayerList(),
Callback = function(v)
if v == "è‡ªåˆ†" then
WingsTargetPlayer = nil
else
WingsTargetPlayer = Players:FindFirstChild(v)
end
end
})

